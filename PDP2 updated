import openpyxl 
from openpyxl import load_workbook
wb=openpyxl.load_workbook(r'C:\Users\jamesk\Downloads\sample EGWP PDP report.xlsx')
wb2=openpyxl.load_workbook(r'C:\Users\jamesk\Downloads\EGWP Weekly PDP Report Template.xlsx')
wb3=openpyxl.load_workbook(r'L:\Death Master\PGold DOD Reports\Deceased Members 11-06-2023.xlsx')
sheet= wb['Sheet1']
sheet2 = wb2['Details']
sheet4=wb2['PGOLD Death']
sheet3= wb3['Deceased Members']

rows=20000
columns = 66 

listab=[]
for i in range(1,rows+1):
    listab.append([])

    #copy sheet 
for r in range(1,rows+1):
    for c in range (1,columns+1):
        e=sheet.cell(row=r,column=c)
        listab[r-1].append(e.value)
    #paste sheet to sheet2 details
for r in range(1,rows+1):
    for c in range(1,columns+1):
        j=sheet2.cell(row=r+1,column=c)
        j.value=listab[r-1][c-1]

