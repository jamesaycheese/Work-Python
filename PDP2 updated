import openpyxl 
from openpyxl import load_workbook
wb=openpyxl.load_workbook(r'L:\ESI\PDP Weekly Reports\2023 Reports\2023 11 07 EGWP PDP report.xlsx')
wb2=openpyxl.load_workbook(r'C:\Users\JamesK\Downloads\test.xlsx')


sheet= wb['PDP Death']
sheet2 = wb2['Sheet1']

rows=50
columns = 66 

listab=[]
for i in range(1,rows+1):
    listab.append([])
    #copy d
for r in range(1,rows+1):
    for c in range (1,columns+1):
        e=sheet2.cell(row=r,column=c)
        listab[r-1].append(e.value)
    #paste d
for r in range(1,rows+1):
    for c in range(1,columns+1):
        j=sheet.cell(row=r,column=c)
        j.value=listab[r-1][c-1]
        
        
sheet.delete_cols(32,66)
sheet.delete_cols(1,15)
sheet['Q2'] ="=VLOOKUP(A2,'PGOLD Death'!$A$2:$D$1000,4,FALSE)"

wb.save(r'L:\ESI\PDP Weekly Reports\2023 Reports\2023 11 07 EGWP PDP report.xlsx')
wb2['Sheet1'].delete_rows(1, sheet.max_row)

